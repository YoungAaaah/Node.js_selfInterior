<form method="POST" action="/users/supplier_signup">
    <div class="container col-md-5" style="margin-top: 3%; margin-bottom : 3%">
        <h3 style="margin-bottom: 10%">공급업체 회원가입</h3>
        <h5 style="float:left; margin-right: 5%; width: 150px;">아이디 </h5> <input type="text" name="supplier_id"class="form-control" style="width:40%; margin-bottom: 5%"/>
        <h5 style="float:left; margin-right: 5%; width: 150px;">비밀번호 </h5> <input type="password" name="supplier_pw" class="form-control" style="width:40%; margin-bottom: 5%"/>
        <h5 style="float:left; margin-right: 5%; width: 150px;">업체명 </h5> <input type="text" name="supplier_name" class="form-control" style="width:40%; margin-bottom: 5%"/>
        <h5 style="float:left; margin-right: 5%; width: 150px;">사업자 등록번호 </h5> <input type="text" name="cr_num"  class="form-control" style="width:40%; margin-bottom: 5%"/>
        <h5 style="float:left; margin-right: 5%; width: 150px;">대표자 명 </h5> <input type="text" name="manager" name="id" class="form-control" style="width:40%; margin-bottom: 5%"/>
        <h5 style="float:left; margin-right: 5%; width: 150px;">대표 전화번호 </h5> <input type="text" name="phone_num" class="form-control" style="width:40%; margin-bottom: 5%" placeholder="***-****-****"/>
        <h5 style="float:left; margin-right: 5%; width: 150px;">대표 이메일 </h5> <input type="email" name="supplier_email" class="form-control" style="width:40%; margin-bottom: 5%" />
        <input class="btn blue1" type="submit" style="width: 100%" value="완료"/>
    </div>
</form>